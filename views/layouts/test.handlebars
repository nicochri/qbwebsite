<html>
    <head>
        <script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jquery/1.9.0/jquery.min.js"></script>
        <script type="text/javascript" src="js/handlebars.js"></script>
        <script type="text/javascript" src="js/templates.js"></script>
        <script type="text/javascript">
        var phonebook = {
            people:[
                {
                    name : "George",
                    phoneNumber : "888-888-8888"
                },
                {
                    name : "Larry",
                    phoneNumber : "555-555-5555"
                }
            ]
        }
        
        var renderPhonebook = function(){
            $('body').append(
                Templates["phonebook"](phonebook) //Render it!
            );
        }
        
        /* Extra crap to make sure the templates are loaded. 
        There are better ways of doing this, such as requirejs */
        
        if(!Templates.loaded){
            Templates.onLoaded = renderPhonebook();
        }
        else{
            renderPhonebook();
        }
        
        </script>
    </head>
    <body>
      
    </body>
</html>